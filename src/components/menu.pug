mixin menu(mod)
    nav(class="menu menu--"+mod)
        ul.menu__list
            -var arr = ["Главная", "Обо мне", "Навыки", "Работы", "Отзывы", "Связаться"];
            each item in arr
                li.menu__item
                    a(href="#", target="_blank").menu__link #{item}
                    